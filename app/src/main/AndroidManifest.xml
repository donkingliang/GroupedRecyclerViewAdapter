<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.donkingliang.groupedadapterdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.GroupListActivity" />
        <activity android:name=".activity.NoHeaderActivity" />
        <activity android:name=".activity.NoFooterActivity" />
        <activity android:name=".activity.Grid1Activity" />
        <activity android:name=".activity.Grid2Activity" />
        <activity android:name=".activity.ExpandableActivity" />
        <activity android:name=".activity.VariousActivity" />
        <activity android:name=".activity.VariousChildActivity" />

    </application>

</manifest>